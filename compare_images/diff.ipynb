    "# import module\n",
    "from PIL import Image, ImageChops\n",
    "\n",
    "# assign images\n",
    "img1 = Image.open(\"kakka1.jpg\")\n",
    "img2 = Image.open(\"kakka2.jpg\")\n",
    "\n",
    "# finding difference\n",
    "diff = ImageChops.difference(img1, img2)\n",
    "\n",
    "# showing the difference\n",
    "diff.show()\n"
   ]
